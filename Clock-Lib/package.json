{
  "name": "clock-lib",
  "version": "2.0.0",
  "type": "module",
  "description": "Fully customizable clock widget for Wallpaper Engine web wallpapers",
  "main": "dist/clock.js",
  "module": "dist/clock.js",
  "types": "dist/clock.d.ts",
  "exports": {
    ".": {
      "import": "./dist/clock.js",
      "types": "./dist/clock.d.ts"
    },
    "./style.css": "./dist/clock.css"
  },
  "files": [
    "dist",
    "clock-properties.json"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "pinia": "^2.1.0",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.0"
  },
  "keywords": [
    "wallpaper-engine",
    "clock",
    "widget",
    "vue3",
    "typescript"
  ],
  "author": "DeisDev",
  "license": "MIT"
}
